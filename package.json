{
  "name": "superficial",
  "version": "0.2.0",
  "description": "A library for managing responsive inline styles with React",
  "repository": {
    "type": "git",
    "url": "https://github.com/gisikw/superficial.git"
  },
  "main": "dist/index.js",
  "homepage": "https://github.com/gisikw/superficial",
  "bugs": "https://github.com/gisikw/superficial/issues",
  "scripts": {
    "test": "babel-tape-runner 'test/**/*.js' | tap-min",
    "test:coverage": "babel-tap --cov --coverage-report=lcov 'test/**/*.js'",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build",
    "lint": "eslint src test"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-tap": "^5.0.0",
    "babel-tape-runner": "^2.0.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "tap-min": "^1.1.0",
    "tape": "^4.6.3"
  }
}
